tdata)\r\n\t\t\t\tif(this.textdata.name.length>18){\r\n\t\t\t\t\tthis.$message({\r\n\t\t\t\t\t\tmessage: '昵称不符合规范',\r\n\t\t\t\t\t\ttype: 'error',\r\n\t\t\t\t\t\tshowClose: true,\r\n\t\t\t\t\t});\r\n\t\t\t\t\treturn 0;\r\n\t\t\t\t}\r\n\t\t\t\tif(this.textdata.age>200||this.textdata.age<0){\r\n\t\t\t\t\tthis.$message({\r\n\t\t\t\t\t\tmessage: '年龄不符合规范',\r\n\t\t\t\t\t\ttype: 'error',\r\n\t\t\t\t\t\tshowClose: true,\r\n\t\t\t\t\t});\r\n\t\t\t\t\treturn 0;\r\n\t\t\t\t}\r\n\t\t\t\tvar reg=/^[0-9]*$/;\r\n\t\t\t\tif(!reg.test(this.textdata.age)){\r\n\t\t\t\t\tthis.$message({\r\n\t\t\t\t\t\tmessage: '请输入数字',\r\n\t\t\t\t\t\ttype: 'error',\r\n\t\t\t\t\t\tshowClose: true,\r\n\t\t\t\t\t});\r\n\t\t\t\t\treturn 0;\r\n\t\t\t\t}\r\n\t\t\t\thttp({\r\n\t\t\t\t\turl: '/user/update?username=' + this.textdata.account + \"&name=\" + this.textdata.name +\r\n\t\t\t\t\t\t\"&sex=\" + this.textdata.sex + \"&age=\" + this.textdata.age,\r\n\t\t\t\t\tmethod: 'get'\r\n\t\t\t\t}).then(res => {\r\n\t\t\t\t\tif (res.data == \"修改成功\") {\r\n\t\t\t\t\t\tthis.$message({\r\n\t\t\t\t\t\t\tmessage: '保存成功',\r\n\t\t\t\t\t\t\ttype: 'success',\r\n\t\t\t\t\t\t\tshowClose: true,\r\n\t\t\t\t\t\t});\r\n\t\t\t\t\t} else {\r\n\t\t\t\t\t\tthis.$message({\r\n\t\t\t\t\t\t\tmessage: '保存失败',\r\n\t\t\t\t\t\t\ttype: 'error',\r\n\t\t\t\t\t\t\tshowClose: true,\r\n\t\t\t\t\t\t});\r\n\t\t\t\t\t}\r\n\r\n\t\t\t\t})\r\n\r\n\t\t\t},\r\n\t\t\tdelsongsheet(idsongsheet) {\r\n\t\t\t\tthis.$confirm('是否确定删除？', '提示', {\r\n\t\t\t\t\tconfirmButtonText: '确定',\r\n\t\t\t\t\tcancelButtonText: '取消',\r\n\t\t\t\t\ttype: 'success'\r\n\t\t\t\t}).then(() => {\r\n\t\t\t\t\thttp({\r\n\t\t\t\t\t\turl: '/songsheet/del?idsongsheet=' + idsongsheet,\r\n\t\t\t\t\t\tmethod: 'get'\r\n\t\t\t\t\t}).then(res => {\r\n\t\t\t\t\t\tif (res.data == '删除成功') {\r\n\t\t\t\t\t\t\tthis.getsongsheet();\r\n\t\t\t\t\t\t\tthis.$message({\r\n\t\t\t\t\t\t\t\tmessage: '删除成功',\r\n\t\t\t\t\t\t\t\ttype: 'success',\r\n\t\t\t\t\t\t\t\tshowClose: true,\r\n\t\t\t\t\t\t\t});\r\n\t\t\t\t\t